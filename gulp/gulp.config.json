{
    "distribution_folder": "./dist",
    "js": {
        "uglify": true,
        "src": "./src/assets/js/",
        "dest": "/assets/js/"
    },
    "copy": {
        "src": "./src/assets/"
    },
    "pug": {
        "src": "./src/pug/",
        "dest": ""
    },
    "sass": {
        "logError": true,
        "sourcemaps": true,
        "atuoprefix": true,
        "minify": true,
        "mainFile": "main",
        "src": "./src/sass/",
        "dest": "/assets/css/"
    }
}