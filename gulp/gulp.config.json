{
    "distribution_folder": "./dist",
    "js": {
        "uglify": true,
        "src": "./src/js/",
        "dest": "/js/"
    },
    "copy": {
        "src": "./src/assets/"
    },
    "pug": {
        "src": "./src/pug/",
        "dest": "",
        "mainFile": "index.pug",
        "data": {
            "title": "Start Template",
            "css_to_include": [
                "/css/main.css"
            ],
            "js_to_include": [
                "/js/main.js"
            ]
        }
    },
    "sass": {
        "logError": true,
        "sourcemaps": true,
        "atuoprefix": true,
        "minify": true,
        "mainFile": "main",
        "src": "./src/sass/",
        "dest": "/css/"
    }
}